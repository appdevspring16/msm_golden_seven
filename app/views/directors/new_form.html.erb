<h1>Add Director</h1>

<form action="/create_director">
  <div>
    <label for="name">
      Name
    </label>
    <input type="text" id="name" name="name" placeholder="Name">
  </div>

  <div>
    <label for="dob">
      DOB
    </label>
    <input type="text" id="dob" name="dob" placeholder="DOB">
  </div>

  <div>
    <label for="bio">
      Bio
    </label>
    <input type="text" id="bio" name="bio" placeholder="Bio">
  </div>

  <div>
    <label for="image_url">
      Image URL
    </label>
    <input type="text" id="image_url" name="image_url" placeholder="Image URL">
  </div>

  <div>
    <button>Create Director</button>
  </div>
</form>
