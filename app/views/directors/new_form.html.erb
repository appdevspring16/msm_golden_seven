<h1>Add Director</h1>

<form action="/create_director">
  <label for="dob">
    Dob
  </label>
  <input type="text" id="dob" name="dob" placeholder="Dob">


  <label for="name">
    Name
  </label>
  <input type="text" id="name" name="name" placeholder="Name">

  <label for="bio">
    Bio
  </label>
  <textarea id="bio" name="bio" placeholder="Bio" rows="3"></textarea>

  <label for="image_url">
    Image url
  </label>
  <input type="text" id="image_url" name="image_url" placeholder="Image url">

  <button>
    <input type="submit" value="Create Director">
  </button>

</form>
