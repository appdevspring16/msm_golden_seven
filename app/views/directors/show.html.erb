<div class="page-header">
  <h1>Director: <%= @director.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Dob</dt>
  <dd><%= @director.dob %></dd>

  <dt>Name</dt>
  <dd><%= @director.name %></dd>

  <dt>Bio</dt>
  <dd><%= @director.bio %></dd>

  <dt>Image</dt>
