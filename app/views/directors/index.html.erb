<h1>Directors</h1>

<a href="#">Add a New Director</a>

<table>
  <tr>
    <th>Image</th>
    <th>Name</th>
    <th>Dob</th>
    <th>Bio</th>
    <th colspan="3">Actions</th>
  </tr>
  <% @director_list.each do |director| %>
  <tr>
    <td>
      <img src="<%= director.image_url %>" width = "80">
    </td>
    <td>
      <%= director.name %>
    </td>
    <td>
      <%= director.dob %>
    </td>
    <td>
      <%= director.bio %>
    </td>
    <td>
      <a href="#">Show</a>
    </td>
    <td>
      <a href="#">Edit</a>
    </td>
    <td>
      <a href="#">Delete</a>
    </td>
  </tr>
  <% end %>
</table>
