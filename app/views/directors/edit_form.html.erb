<h1>Edit Director</h1>


<form action="/update_director/<%= @director.id %>">
  <label for="director_dob">Dob</label>
  <input type="text" name="director_dob" value="<%= @director.dob %>">
  <label for="director_name">Name</label>
  <input type="text" name="director_name" value="<%= @director.name %>">
  <label for="director_bio">Bio</label>
  <input type="text" name="director_bio" value="<%= @director.bio %>">
    <label for="director_url">Image url</label>
  <input type="text" name="director_url" value="<%= @director.image_url %>">
  <button>Create Director</button>
</form>
