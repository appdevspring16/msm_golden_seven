<h1>Director <%= @director.id %></h1>

<table>
  <tr>
    <td><p>Dob</p></td>
    <td><%= @director.dob %></td>
  </tr>
  <tr>
    <td>Name</td>
    <td><%= @director.name %></td>
  </tr>
  <tr>
    <td>Bio</td>
    <td><%= @director.bio %></td>
  </tr>
  <tr>
    <td>Image</td>
    <td><img src="<%= @director.image_url %>" width="250"></td>
  </tr>
</table>
