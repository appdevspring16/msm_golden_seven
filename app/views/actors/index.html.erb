<h1>Actors</h1>
<hr>
<a href="/actors/new_form" class="btn btn-primary">ADD A NEW ACTOR</a>
<table class="table">
  <th>Image</th>
  <th>Dob</th>
  <th>Name</th>
  <th>Bio</th>
  <th></th>
  <th></th>
  <th></th>
<tbody>
<% @actors.each do |actor| %>
<tr>
  <td><img src="<%=actor.image_url%>" alt="" /></td>
  <td><%=actor.dob%></td>
  <td><%=actor.name%></td>
  <td><%=actor.bio%></td>
  <td><a href="/actors/<%=actor.id%>" class="btn btn-primary"></a></td>
  <td><a href="/actors/<%=actor.id%>/edit_form" class="btn btn-warning"></a></td>
  <td><a href="/delete_actor/<%=actor.id%>" class="btn btn-danger"></a></td>
</tr>

<%end%>

</tbody>





</table>
