<h1>Actors</h1>

<a href="#">Add a New Actor</a>

<table>
  <tr>
    <th>Image</th>
    <th>Name</th>
    <th>Dob</th>
    <th>Bio</th>
    <th colspan="3">Actions</th>
  </tr>
  <% @actor_list.each do |actor| %>
  <tr>
    <td>
      <img src="<%= actor.image_url %>" width = "80">
    </td>
    <td>
      <%= actor.name %>
    </td>
    <td>
      <%= actor.dob %>
    </td>
    <td>
      <%= actor.bio %>
    </td>
    <td>
      <a href="#">Show</a>
    </td>
    <td>
      <a href="#">Edit</a>
    </td>
    <td>
      <a href="#">Delete</a>
    </td>
  </tr>
  <% end %>
</table>
