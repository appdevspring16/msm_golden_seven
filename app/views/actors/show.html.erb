<div class="page-header">
  <h1>Actor: <%= @actor.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Dob</dt>
  <dd><%= @actor.dob %></dd>

  <dt>Name</dt>
  <dd><%= @actor.name %></dd>

  <dt>Bio</dt>
  <dd><%= @actor.bio %></dd>

  <dt>Image</dt>
  <dd><img src="<%= @actor.image_url %>" width="320"></dd>

<td>
  <a href="/actors/<%= @actor.id %>/edit_form">Edit</a>
</td>
<td>
  <a href="/delete_actor/<%= @actor.id %>">Delete</a>
</td>
<td>
  <a href="/actors">Go back to Actors Index</a>
</td>

</dl>
