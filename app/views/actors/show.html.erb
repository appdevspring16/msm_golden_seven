<h1>Actor #<%= @actor.id %></h1>

<table>
  <tr>
    <td>
      <img src="<%= @actor.image_url %>" width="320">
    </td>
    <td>
      <%= @actor.dob %>
    </td>
    <td>
      <%= @actor.name %>
    </td>
    <td>
      <%= @actor.bio %>
    </td>
  </tr>
</table>

<a href="/actors">Back</a>
<a href="/actors/<%= @actor.id %>/edit_form">Edit</a>
<a href="/delete_actor/<%= @actor.id %>">Delete</a>
