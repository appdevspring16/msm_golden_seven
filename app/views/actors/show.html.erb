<h1>Actor # <%= @actor.id %></h1>

<dl>
  <dt><h4>Name</h4></dt>
  <dd><%= @actor.name %></dd>

  <dt><h4>DOB</h4></dt>
  <dd><%= @actor.dob %></dd>

  <dt><h4>Bio</h4></dt>
  <dd><%= @actor.bio %></dd>

  <dt><h4>Image</h4></dt>
  <dd><img src="<%= @actor.image_url %>" width = "320"></dd>
</dl>

<a href="/delete_actor/<%= @actor.id %>"></a>
