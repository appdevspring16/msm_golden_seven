<h1>Add A Movie</h1>

<form action="http://localhost:3000/create_movie">
  <label>Title</label>
  <input type="text" name="the_title">

  <label>Year</label>
  <input type="text" name="the_year">

  <label>Duration</label>
  <input type="text" name="the_duration">

  <label>Description</label>
  <input type="text" name="the_description">

  <label>Image URL</label>
  <input type="text" name="the_image_url">

  <button>Create Movie</button>
</form>
